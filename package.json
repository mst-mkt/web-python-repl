{
  "name": "vite-react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "biome lint ./src/",
    "lint:fix": "biome lint --apply ./src/",
    "format": "biome format --write ./src/",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@master/css": "2.0.0-rc.26",
    "@master/css-extractor.vite": "2.0.0-rc.26",
    "@tabler/icons-react": "^3.1.0",
    "isomorphic-fetch": "^3",
    "pyodide": "^0.25.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.2",
    "@types/react": "^18.2.69",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "typescript": "^5.4.3",
    "vite": "^5.2.4"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "biome check --apply"
    ]
  }
}
