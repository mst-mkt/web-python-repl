{
  "name": "web-python-repl",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "biome lint ./src/",
    "lint:fix": "biome lint --apply ./src/",
    "format": "biome format --write ./src/",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@master/css": "2.0.0-rc.40",
    "@master/css-extractor.vite": "2.0.0-rc.41",
    "@master/normal.css": "^3.1.0",
    "@tabler/icons-react": "^3.6.0",
    "isomorphic-fetch": "^3",
    "pyodide": "^0.26.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "typescript": "^5.4.5",
    "vite": "^5.2.13"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "biome check --apply"
    ]
  }
}
